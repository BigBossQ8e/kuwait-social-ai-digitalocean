# Environment variables for Kuwait Social AI Frontend

# API Configuration
VITE_API_URL=http://localhost:5000
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_TITLE=Kuwait Social AI
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_PRAYER_TIMES=true
VITE_ENABLE_COMPETITOR_ANALYSIS=true
VITE_ENABLE_AI_CONTENT_GENERATION=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEV_TOOLS=true

# Kuwait-specific Configuration
VITE_DEFAULT_LANGUAGE=en
VITE_SUPPORTED_LANGUAGES=en,ar
VITE_DEFAULT_TIMEZONE=Asia/Kuwait
VITE_DEFAULT_LOCATION=Kuwait City

# External Services
VITE_PRAYER_TIMES_API=https://api.aladhan.com/v1
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key_here

# Analytics and Monitoring
VITE_ENABLE_ERROR_REPORTING=true
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_GA_TRACKING_ID=your_google_analytics_id_here

# Security
VITE_ENABLE_CSP=true
VITE_ALLOWED_HOSTS=localhost,127.0.0.1

# Performance
VITE_ENABLE_SW=true
VITE_CACHE_DURATION=300000

# Development
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_LOG_LEVEL=debug